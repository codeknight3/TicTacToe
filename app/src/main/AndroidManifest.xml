<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tic_tac_toe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FirstPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mainpage" />
        <activity android:name=".LastPage" />
        <activity android:name=".MainPage2" />
        <activity android:name=".PlaySelection" />
        <activity android:name=".Player1wins" />
        <activity android:name=".Player2wins" />
        <activity android:name=".Draw" />
        <activity android:name=".RandomGame"></activity>
    </application>

</manifest>